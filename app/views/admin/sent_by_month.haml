- @crumbtrail = link_to_admin_home
- @title = "#{@month.to_s(:month_year)} Sent Messages"

%table.text.column_content
  %tr
    %th Constituency
    %th Message count
  - @sent_by_constituency.each do |constituency_name, messages|
    %tr
      %td= link_to_edit_constituency constituency_name
      %td= messages.size

%p.text
  = '&nbsp;'
%p.text
  = link_to 'Sent messages', url_for(:controller=>'admin',:action=>'sent')
