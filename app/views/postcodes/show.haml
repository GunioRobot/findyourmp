%html
  %head
    %title= @postcode.code
  %body
    %div
      %p
        = "postcode: #{@postcode.code}"
        %br
        = "constituency_id: #{@postcode.constituency_id}"
        %br
        = "constituency: #{@postcode.constituency_name}"
        %br
        - if @postcode.member_name
          = "member: #{@postcode.member_name}"
        - else
          = "constituency is vacant"

